{
  "name": "@velozzity/ts-helpers",
  "version": "0.0.2",
  "description": "A set of typescript helper functions to be used in common velozzity packages.",
  "main": "dist",
  "scripts": {
    "build": "npx tsc",
    "watch": "npx tsc --watch",
    "test": "jest --config jestconfig.json",
    "format": "prettier --write \"src/**/*.ts\" ",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build"
  },
  "files": [
    "dist/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/velozzity/ts-helpers.git"
  },
  "keywords": [
    "helpers",
    "typescript",
    "ts"
  ],
  "author": "thedarkkid.codes@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/velozzity/ts-helpers/issues"
  },
  "homepage": "https://github.com/velozzity/ts-helpers#readme",
  "dependencies": {
    "chalk": "^4.1.0",
    "crypto": "^1.0.1",
    "ejs": "^3.1.6",
    "figlet": "^1.5.0",
    "fs-extra": "^9.1.0",
    "path": "^0.12.7",
    "sanitize-html": "^2.3.2",
    "user": "0.0.0"
  },
  "devDependencies": {
    "@types/ejs": "^3.0.6",
    "@types/figlet": "^1.2.1",
    "@types/fs-extra": "^9.0.8",
    "@types/jest": "^26.0.20",
    "@types/sanitize-html": "^1.27.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.2.3"
  }
}
